{
    "env": {
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2015
    },
    "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "single"],
      "no-unused-vars": ["error", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }],
      "strict": ["error", "global"],
      "no-trailing-spaces": ["error"]
    }
  }